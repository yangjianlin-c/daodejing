           \@@VECTORMATRIXPRODUCT(#1)(#2)(#3)
        \else \@@@VECTORMATRIXPRODUCT(#1)(#2)(#3)\fi\@OUTPUTVECTOR(#3)}

\def\@@SCALARMATRIXPRODUCT#1(#2;#3)(#4,#5;#6,#7){%
       \SCALARVECTORPRODUCT{#1}(#2)(#4,#5)
       \SCALARVECTORPRODUCT{#1}(#3)(#6,#7)}

\def\@@@SCALARMATRIXPRODUCT#1(#2;#3;#4){%
